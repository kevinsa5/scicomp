# slow

isPrime = lambda(n){
    flag = true;
    if(n > 3){
        if(n % 2 == 0){
            flag = false;
        } else {
            for(i in 3:2:(n/2)){
                if(n % i == 0){
                    flag = false;
                    break;
                };
            };
        };
    };    
    flag;
};

n = 0;
N = 10001;
x = 1;

while(n < N){
    x = x + 1;
    while(isPrime(x) == false){
        x = x+1;
    };
    n = n + 1;
};
println(x);
